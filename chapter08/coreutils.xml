          <para>Преобразует пробелы в табуляции</para>
          <indexterm zone="ch-system-coreutils unexpand">
            <primary sortas="b-unexpand">unexpand</primary>
          </indexterm>
        </listitem>
      </varlistentry>

      <varlistentry id="uniq">

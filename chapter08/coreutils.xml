          <para>Сообщает имена пользователей, вошедших в систему в данный момент</para>
          <indexterm zone="ch-system-coreutils users">
            <primary sortas="b-users">users</primary>
          </indexterm>
        </listitem>
      </varlistentry>

      <varlistentry id="vdir">
